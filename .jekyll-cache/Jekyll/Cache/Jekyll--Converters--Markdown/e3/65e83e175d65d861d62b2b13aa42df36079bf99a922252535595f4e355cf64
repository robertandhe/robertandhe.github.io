<span class="n">LogisticRegression</span><span class="o">::</span><span class="n">LogisticRegression</span><span class="p">(</span><span class="kt">double</span> <span class="n">learningRate</span><span class="o">=</span><span class="mf">0.001</span><span class="p">,</span> <span class="kt">int</span> <span class="n">iteration</span><span class="o">=</span><span class="mi">500</span><span class="p">)</span>
