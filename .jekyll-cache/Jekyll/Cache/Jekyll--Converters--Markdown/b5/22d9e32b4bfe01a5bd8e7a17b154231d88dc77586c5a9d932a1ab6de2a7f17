<span class="kt">bool</span> <span class="n">WordEngine</span><span class="o">::</span><span class="n">setBatchMarks</span><span class="p">(</span><span class="n">QStringList</span> <span class="o">&amp;</span><span class="n">itemList</span><span class="p">,</span><span class="n">QStringList</span> <span class="n">sometexts</span><span class="p">)</span>
<span class="p">{</span>
    <span class="n">QAxObject</span><span class="o">*</span> <span class="n">allBookmarks</span> <span class="o">=</span> <span class="nb">NULL</span><span class="p">;</span>
    <span class="n">allBookmarks</span> <span class="o">=</span> <span class="n">m_wordDocuments</span><span class="o">-&gt;</span><span class="n">querySubObject</span><span class="p">(</span><span class="s">"Bookmarks"</span><span class="p">);</span>
