<span class="kt">bool</span> <span class="n">WordEngine</span><span class="o">::</span><span class="n">open</span><span class="p">(</span><span class="kt">bool</span> <span class="n">bVisable</span><span class="p">)</span>
<span class="p">{</span>
    <span class="c1">//æ–°å»ºä¸€ä¸ªwordåº”ç”¨ç¨‹åº,å¹¶è®¾ç½®ä¸ºå¯è§</span>
    <span class="n">m_wordWidget</span> <span class="o">=</span> <span class="k">new</span> <span class="n">QAxObject</span><span class="p">();</span>
    <span class="kt">bool</span> <span class="n">bFlag</span> <span class="o">=</span> <span class="n">m_wordWidget</span><span class="o">-&gt;</span><span class="n">setControl</span><span class="p">(</span> <span class="s">"word.Application"</span> <span class="p">);</span>
    <span class="k">if</span><span class="p">(</span><span class="o">!</span><span class="n">bFlag</span><span class="p">)</span>
    <span class="p">{</span>
        <span class="c1">// ç”¨wpsæ‰“å¼€</span>
        <span class="n">bFlag</span> <span class="o">=</span> <span class="n">m_wordWidget</span><span class="o">-&gt;</span><span class="n">setControl</span><span class="p">(</span> <span class="s">"kwps.Application"</span> <span class="p">);</span>
