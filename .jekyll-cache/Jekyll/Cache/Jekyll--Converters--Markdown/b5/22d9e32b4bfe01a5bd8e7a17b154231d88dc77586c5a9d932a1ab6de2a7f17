<span class="kt">bool</span> <span class="n">WordEngine</span><span class="o">::</span><span class="n">close</span><span class="p">()</span>
<span class="p">{</span>
<span class="c1">//    qDebug()&lt;&lt;m_bOpened;</span>
    <span class="k">if</span> <span class="p">(</span><span class="n">m_bOpened</span><span class="p">)</span>
    <span class="p">{</span>
        <span class="k">if</span><span class="p">(</span><span class="n">m_wordDocuments</span><span class="p">)</span>
            <span class="n">m_wordDocuments</span><span class="o">-&gt;</span><span class="n">dynamicCall</span><span class="p">(</span><span class="s">"Close (bool)"</span><span class="p">,</span> <span class="nb">true</span><span class="p">);</span><span class="c1">//å…³é—­æ–‡æœ¬çª—å£</span>
        <span class="k">if</span><span class="p">(</span><span class="n">m_wordWidget</span><span class="p">)</span>
            <span class="n">m_wordWidget</span><span class="o">-&gt;</span><span class="n">dynamicCall</span><span class="p">(</span><span class="s">"Quit()"</span><span class="p">);</span><span class="c1">//é€€å‡ºword</span>
        <span class="k">if</span><span class="p">(</span><span class="n">m_wordDocuments</span><span class="p">)</span>
            <span class="k">delete</span> <span class="n">m_wordDocuments</span><span class="p">;</span>
        <span class="k">if</span><span class="p">(</span><span class="n">m_wordWidget</span><span class="p">)</span>
            <span class="k">delete</span> <span class="n">m_wordWidget</span><span class="p">;</span>
        <span class="n">m_bOpened</span> <span class="o">=</span> <span class="nb">false</span><span class="p">;</span>
    <span class="p">}</span>

    <span class="k">return</span> <span class="n">m_bOpened</span><span class="p">;</span>
<span class="p">}</span>

