    <span class="c1">//è·å–æ‰€æœ‰çš„å·¥ä½œæ–‡æ¡£</span>
    <span class="n">QAxObject</span> <span class="o">*</span><span class="n">document</span> <span class="o">=</span> <span class="n">m_wordWidget</span><span class="o">-&gt;</span><span class="n">querySubObject</span><span class="p">(</span><span class="s">"Documents"</span><span class="p">);</span>
